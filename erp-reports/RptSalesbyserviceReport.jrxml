		<queryString language="SQL">
			<![CDATA[SELECT distinct appointment_schesule.booking_date,
group_concat(distinct service.service_name)as service_Name,
count(distinct service.service_name)as item_Sold,
sum(distinct order_details.unit_price) as gross_sales,
sum(distinct sales_order.discount_price) as discount,
(sum(distinct order_details.unit_price)-sum(distinct sales_order.discount_price)) as net_sales,
order_details.gst,
(((sum(distinct order_details.unit_price)-sum(distinct sales_order.discount_price))*order_details.gst)/100) +
(sum(distinct order_details.unit_price)-sum(distinct sales_order.discount_price)) as total_sales

 FROM store_appointmentschedule as appointment_schesule 
 inner join sales_salesorderrequest as sales_order on sales_order.customer_id=appointment_schesule.customer_id
 inner join sales_salesorderdetails as order_details on order_details.po_order_id=sales_order.id
 inner join store_storeservices as service on service.id=order_details.service_id
 where appointment_schesule.booking_date between  $P{From_Date} and  $P{To_Date} 
 group by appointment_schesule.booking_date,order_details.gst]]>
		</queryString>
