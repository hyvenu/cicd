		<parameter name="From_Date" class="java.sql.Date">
			<parameterDescription><![CDATA[Enter From Date]]></parameterDescription>
		</parameter>
		<parameter name="To Date" class="java.sql.Date">
			<parameterDescription><![CDATA[Enter To Date]]></parameterDescription>
		</parameter>
		<queryString language="SQL">
			<![CDATA[SELECT distinct Appointment_Schedule.booking_date,
count(distinct Price.qty) as Item_Sold, sum(distinct Price.unit_price) as Gross_Sales,
 sum(Order_Request.discount_price)as Discount,
 sum(distinct Price.unit_price)-sum(Order_Request.discount_price) as Net_Sales, Price.tax,
((sum(distinct Price.unit_price)-sum(Order_Request.discount_price))*(Price.tax))/100 +
sum(distinct Price.unit_price)-sum(Order_Request.discount_price) as Total_Sales

FROM d5n.store_appointmentschedule as Appointment_Schedule

inner join sales_salesorderrequest as Order_Request on Order_Request.customer_id=Appointment_Schedule.customer_id
inner join sales_salesorderdetails as Sales_Order on Sales_Order.po_order_id=Order_Request.id
 left join inventory_productpricemaster as Price on Price.product_id=Sales_Order.product_id
 where Sales_Order.product_id is not null
 and Appointment_Schedule.booking_date between  $P{From_Date} and  $P{To Date} 
group by Appointment_Schedule.booking_date, Price.product_id is not null]]>
		</queryString>
